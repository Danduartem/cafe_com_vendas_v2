# Production Environment Configuration
# This file is loaded automatically when NODE_ENV=production

# Ensure production mode
NODE_ENV=production

# Production optimization flags
VITE_DEV_MODE=false
VITE_DEBUG_MODE=false

# Production asset optimization
VITE_COMPRESS_ASSETS=true
VITE_MINIFY_ASSETS=true

# Production build configuration
SKIP_ESLINT=false
SKIP_TYPECHECK=false

# Production performance settings
VITE_GENERATE_SOURCEMAP=false
VITE_ANALYZE_BUNDLE=false

# Production debugging (disabled)
DEBUG_VITE=false
DEBUG_ELEVENTY=false